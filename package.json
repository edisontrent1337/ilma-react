{
  "name": "@edisontrent1337/web-react",
  "version": "0.4.3",
  "description": "Provides different React Components for web development.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run copy-resources",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy-resources": "cpx './src/**/*.{css,gif}' dist/src"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edisontrent1337/web-react.git"
  },
  "keywords": [
    "ui",
    "react"
  ],
  "author": "Sinthujan Thanabalasingam",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/edisontrent1337/web-react/issues"
  },
  "homepage": "https://github.com/edisontrent1337/web-react#readme",
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-transform-modules-commonjs": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@types/node": "^12.0.5",
    "@types/react": "16.8.19",
    "cpx": "1.5.0",
    "typescript": "3.5.1"
  },
  "dependencies": {
    "@mdi/font": "^3.6.95",
    "material-colors": "^1.2.6"
  }
}
